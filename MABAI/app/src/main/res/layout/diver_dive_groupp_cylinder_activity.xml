<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <data>
        <variable
            name="diverDiveGroupCyl"
            type="ca.myairbuddyandi.DiverDiveGroupCyl"/>
        <import type="android.view.View"/>
    </data>
    <LinearLayout
        android:id="@+id/diver_dive_groupp_cylinder_activity"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@android:color/white"
            android:orientation="horizontal"
            android:weightSum="1">
            <Button
                android:id="@+id/cancel_button"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/button_cancel"
                android:layout_weight=".5"
                android:background="@color/very_light_blue"
                style="attr/borderlessButtonStyle"/>
            <Button
                android:id="@+id/save_button"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/button_save"
                android:layout_weight=".5"
                android:background="@color/very_light_blue"
                style="attr/borderlessButtonStyle"/>
        </LinearLayout>
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
                <com.google.android.material.textfield.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <EditText
                        android:text="@={`` + diverDiveGroupCyl.BeginningPressure}"
                        android:id="@+id/editTextBP"
                        android:inputType="numberDecimal"
                        android:imeOptions="actionNext"
                        android:hint="@string/hint_beginning_pressure"
                        android:gravity="start"
                        android:selectAllOnFocus="true"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:layout_marginEnd="10dp"
                        android:focusable="true"
                        app:addTextChangedListener="@{diverDiveGroupCyl.getOnTextChanged}"
                        tools:ignore="Autofill" />
                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <EditText
                        android:text="@={`` + diverDiveGroupCyl.EndingPressure}"
                        android:id="@+id/editTextEP"
                        android:inputType="numberDecimal"
                        android:imeOptions="actionNext"
                        android:hint="@string/hint_ending_pressure"
                        android:gravity="start"
                        android:selectAllOnFocus="true"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:layout_marginEnd="10dp"
                        app:addTextChangedListener="@{diverDiveGroupCyl.getOnTextChanged}"
                        tools:ignore="Autofill" />
                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <EditText
                        android:text="@={`` + diverDiveGroupCyl.o2}"
                        android:id="@+id/editTextO2"
                        android:inputType="number"
                        android:imeOptions="actionNext"
                        android:hint="@string/hint_oxygen"
                        android:gravity="start"
                        android:selectAllOnFocus="true"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:layout_marginEnd="10dp"
                        app:addTextChangedListener="@{diverDiveGroupCyl.getOnTextChanged}"
                        tools:ignore="Autofill" />
                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <EditText
                        android:text="@={`` + diverDiveGroupCyl.n}"
                        android:id="@+id/editTextN"
                        android:inputType="number"
                        android:imeOptions="actionNext"
                        android:hint="@string/hint_nitrogen"
                        android:gravity="start"
                        android:selectAllOnFocus="true"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:layout_marginEnd="10dp"
                        app:addTextChangedListener="@{diverDiveGroupCyl.getOnTextChanged}"
                        tools:ignore="Autofill" />
                </com.google.android.material.textfield.TextInputLayout>
                <com.google.android.material.textfield.TextInputLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">
                    <EditText
                        android:text="@={`` + diverDiveGroupCyl.he}"
                        android:id="@+id/editTextHE"
                        android:inputType="number"
                        android:imeOptions="actionNext"
                        android:hint="@string/hint_helium"
                        android:gravity="start"
                        android:selectAllOnFocus="true"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:layout_marginEnd="10dp"
                        app:addTextChangedListener="@{diverDiveGroupCyl.getOnTextChanged}"
                        tools:ignore="Autofill" />
                </com.google.android.material.textfield.TextInputLayout>
        </LinearLayout>
    </LinearLayout>
</layout>