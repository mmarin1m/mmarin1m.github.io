<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <data>
        <variable
            name="diveForGraphic"
            type="ca.myairbuddyandi.DiveForGraphic"/>
    </data>
    <LinearLayout
        android:id="@+id/ruleofthirds_activity"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/ic_turnaround_pressure">
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <TableLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                tools:ignore="UselessParent">
            <TableRow
                android:id="@+id/TableRow01"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:stretchColumns="*"
                android:weightSum="10"
                android:paddingTop="4dp"
                android:orientation="horizontal"
                tools:ignore="UselessParent">
                <TextView
                    android:id="@+id/meLbl"
                    android:text="@{diveForGraphic.meLabel}"
                    android:textStyle="bold"
                    android:gravity="center"
                    android:layout_width="0dp"
                    android:layout_weight="3" />
                <TextView
                    android:id="@+id/lblSpacer11"
                    android:text="@string/lbl_blank"
                    android:gravity="center"
                    android:layout_width="0dp"
                    android:layout_weight="1" />
                <TextView
                    android:id="@+id/lblPlan"
                    android:text="@string/lbl_plan"
                    android:textColor="@color/theme_myapp_action_bar"
                    android:gravity="center"
                    android:layout_width="0dp"
                    android:layout_weight="2" />
                <ImageView
                    android:id="@+id/maximize"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="start|center"
                    app:srcCompat="@drawable/ic_plus"
                    app:tint="@color/green"
                    android:contentDescription="@string/cd_maximize"/>
                <TextView
                    android:id="@+id/myBuddyLbl"
                    android:text="@{diveForGraphic.myBuddyFullName}"
                    android:textStyle="bold"
                    android:gravity="center"
                    android:layout_width="0dip"
                    android:layout_weight="3" />
            </TableRow>
            <TableRow
                android:id="@+id/TableRow02"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:stretchColumns="*"
                android:weightSum="8"
                android:orientation="horizontal"
                tools:ignore="UselessParent">
                <TextView
                    android:id="@+id/lblSpacer21"
                    android:text="@string/lbl_blank"
                    android:gravity="center"
                    android:layout_width="0dp"
                    android:layout_weight="1" />
                <TextView
                    android:id="@+id/meTurnaroundLbl"
                    android:text="@{diveForGraphic.myTurnaroundPressureUnit}"
                    android:textStyle="bold"
                    android:gravity="center"
                    android:layout_width="0dp"
                    android:layout_weight="2" />
                <TextView
                    android:id="@+id/lblTurnaround"
                    android:text="@string/lbl_turnaround"
                    android:gravity="center"
                    android:layout_width="0dp"
                    android:layout_weight="2" />
                <TextView
                    android:id="@+id/myBuddyTurnaroundLbl"
                    android:text="@{diveForGraphic.myBuddyTurnaroundPressureUnit}"
                    android:textStyle="bold"
                    android:gravity="center"
                    android:layout_width="0dip"
                    android:layout_weight="2" />
                <TextView
                    android:id="@+id/lblSpacer23"
                    android:text="@string/lbl_blank"
                    android:gravity="center"
                    android:layout_width="0dp"
                    android:layout_weight="1" />
            </TableRow>
            </TableLayout>
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <ca.myairbuddyandi.RuleOfThirdsView
                android:id="@+id/ruleOfThirdsView"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />
        </LinearLayout>
    </LinearLayout>
</layout>