<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable
            name="main"
            type="ca.myairbuddyandi.Main"/>
    </data>
    <androidx.drawerlayout.widget.DrawerLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:id="@+id/activity_main"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fitsSystemWindows="true"
        tools:openDrawer="start">
        <include
            layout="@layout/app_bar_main"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
        <LinearLayout
            android:id="@+id/content_main"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:paddingBottom="@dimen/activity_vertical_margin"
            android:paddingLeft="@dimen/activity_horizontal_margin"
            android:paddingRight="@dimen/activity_horizontal_margin"
            android:paddingTop="120dp"
            app:layout_behavior="@string/appbar_scrolling_view_behavior"
            tools:context="ca.myairbuddyandi.MainActivity"
            tools:showIn="@layout/app_bar_main">
            <TableLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:weightSum="3"
                android:stretchColumns="*"
                tools:ignore="UselessParent">
                <TableRow
                    android:id="@+id/TableRow10"
                    android:orientation="horizontal"
                    android:paddingTop="16dp"
                    android:weightSum="7">
                    <TextView
                        android:id="@+id/meLbl"
                        android:text="@string/lbl_me"
                        android:textStyle="bold"
                        android:color="@color/theme_myapp_action_bar"
                        android:gravity="center"
                        android:layout_width="0dip"
                        android:layout_weight="3" />
                    <TextView
                        android:id="@+id/lblDiveType"
                        android:text="@string/lbl_dive_type"
                        android:textStyle="bold"
                        android:gravity="center"
                        android:layout_width="0dip"
                        android:layout_weight="1" />
                    <TextView
                        android:id="@+id/myBuddyLbl"
                        android:text="@{main.myBuddyName}"
                        android:textStyle="bold"
                        android:gravity="center"
                        android:layout_width="0dip"
                        android:layout_weight="3" />
                </TableRow>
                <TableRow
                    android:id="@+id/TableRow20"
                    android:orientation="horizontal"
                    android:weightSum="3">
                    <TextView
                        android:id="@+id/lblSpacer31"
                        android:text="@string/lbl_blank"
                        android:gravity="center"
                        android:layout_width="0dip"
                        android:layout_weight="1" />
                    <TextView
                        android:id="@+id/diveType"
                        android:text="@{main.diveType}"
                        android:gravity="center"
                        android:layout_width="0dip"
                        android:layout_weight="1"/>
                    <TextView
                        android:id="@+id/lblSpacer32"
                        android:text="@string/lbl_blank"
                        android:gravity="center"
                        android:layout_width="0dip"
                        android:layout_weight="1" />
                </TableRow>
                <TableRow
                    android:id="@+id/TableRow30"
                    android:orientation="horizontal"
                    android:paddingTop="16dp"
                    android:weightSum="3">
                    <TextView
                        android:id="@+id/myGroupp"
                        android:text="@{main.myGroup}"
                        android:gravity="center"
                        android:layout_width="0dip"
                        android:layout_weight="1"/>
                    <TextView
                        android:id="@+id/groupLbl"
                        android:text="@string/lbl_groupp"
                        android:textStyle="bold"
                        android:gravity="center"
                        android:layout_width="0dip"
                        android:layout_weight="1"/>
                    <TextView
                        android:id="@+id/myBuddyGroupp"
                        android:text="@{main.myBuddyGroup}"
                        android:gravity="center"
                        android:layout_width="0dip"
                        android:layout_weight="1"/>
                </TableRow>
                <TableRow
                    android:id="@+id/TableRow40"
                    android:orientation="horizontal"
                    android:weightSum="3">
                    <TextView
                        android:id="@+id/myRmv"
                        android:text="@{main.myRmv}"
                        android:gravity="end"
                        android:paddingStart="0dp"
                        android:paddingEnd="20dp"
                        android:layout_height="match_parent"
                        android:layout_width="0dip"
                        android:layout_weight="1"/>
                    <TextView
                        android:id="@+id/rmvLbl"
                        android:text="@string/lbl_rmv"
                        android:textStyle="bold"
                        android:gravity="center"
                        android:layout_height="match_parent"
                        android:layout_width="0dip"
                        android:layout_weight="1"/>
                    <TextView
                        android:id="@+id/myBuddyRmv"
                        android:text="@{main.myBuddyRmv}"
                        android:gravity="end"
                        android:paddingStart="0dp"
                        android:paddingEnd="20dp"
                        android:layout_height="match_parent"
                        android:layout_width="0dip"
                        android:layout_weight="1"/>
                </TableRow>
                <TableRow
                    android:id="@+id/TableRow50"
                    android:orientation="horizontal"
                    android:paddingTop="16dp"
                    android:weightSum="3">
                    <TextView
                        android:id="@+id/lblMeTotalToDate"
                        android:text="@{main.myTotalToDateX}"
                        android:gravity="end"
                        android:paddingStart="0dp"
                        android:paddingEnd="20dp"
                        android:layout_width="0dip"
                        android:layout_weight="1" />
                    <TextView
                        android:id="@+id/totalToDate"
                        android:text="@string/lbl_total_dive_to_date"
                        android:textStyle="bold"
                        android:gravity="center"
                        android:layout_width="0dip"
                        android:layout_weight="1"/>
                    <TextView
                        android:id="@+id/lblMyBuddyTotalToDate"
                        android:text="@{main.myBuddyTotalToDateX}"
                        android:gravity="end"
                        android:paddingStart="0dp"
                        android:paddingEnd="20dp"
                        android:layout_width="0dip"
                        android:layout_weight="1" />
                </TableRow>
                <TableRow
                    android:id="@+id/TableRow60"
                    android:orientation="horizontal"
                    android:paddingTop="16dp"
                    android:weightSum="3">
                    <TextView
                        android:id="@+id/lblMyLastDive"
                        android:text="@{main.myLastDiveX}"
                        android:gravity="center"
                        android:layout_width="0dip"
                        android:layout_weight="1" />
                    <TextView
                        android:id="@+id/lblLastDive"
                        android:text="@string/lbl_last_dive"
                        android:textStyle="bold"
                        android:gravity="center"
                        android:layout_width="0dip"
                        android:layout_weight="1"/>
                    <TextView
                        android:id="@+id/lblMyBuddyLastDive"
                        android:text="@{main.myBuddyLastDiveX}"
                        android:gravity="center"
                        android:layout_width="0dip"
                        android:layout_weight="1" />
                </TableRow>
                <TableRow
                    android:id="@+id/TableRow70"
                    android:orientation="horizontal"
                    android:weightSum="3">
                    <TextView
                        android:id="@+id/lblMySurfaceInterval"
                        android:text="@{main.mySurfaceInterval}"
                        android:gravity="center"
                        android:layout_width="0dip"
                        android:layout_weight="1" />
                    <TextView
                        android:id="@+id/lblSurfaceInterval"
                        android:text="@string/lbl_surface_interval"
                        android:textStyle="bold"
                        android:gravity="center"
                        android:layout_width="0dip"
                        android:layout_weight="1"/>
                    <TextView
                        android:id="@+id/lblMyBuddySurfaceInterval"
                        android:text="@{main.myBuddySurfaceInterval}"
                        android:gravity="center"
                        android:layout_width="0dip"
                        android:layout_weight="1" />
                </TableRow>
            </TableLayout>
        </LinearLayout>
        <com.google.android.material.navigation.NavigationView
            android:id="@+id/nav_view"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:layout_gravity="start"
            android:fitsSystemWindows="true"
            app:headerLayout="@layout/nav_header_main"
            app:menu="@menu/activity_main_drawer"/>
    </androidx.drawerlayout.widget.DrawerLayout>
</layout>