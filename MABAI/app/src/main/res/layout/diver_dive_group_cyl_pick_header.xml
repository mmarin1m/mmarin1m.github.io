<?xml version="1.0" encoding="utf-8"?>
<layout>
    <androidx.cardview.widget.CardView
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:card_view="http://schemas.android.com/apk/res-auto"
        android:id="@+id/diver_dive_group_cyl_pick_header"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        card_view:cardCornerRadius="5dp"
        android:layout_margin="5dp">
        <TableRow
            android:id="@+id/TableRow01"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="8"
            android:orientation="horizontal">
            <TextView
                android:id="@+id/hdrCylinderType"
                android:text="@string/hdr_type"
                android:gravity="center"
                android:layout_width="0dip"
                android:layout_height="wrap_content"
                android:layout_weight="2" />
            <TextView
                android:id="@+id/hdrUsageType"
                android:text="@string/hdr_usage"
                android:gravity="center"
                android:layout_width="0dip"
                android:layout_height="wrap_content"
                android:layout_weight="2" />
            <TextView
                android:id="@+id/hdrBeginningPressure"
                android:text="@string/hdr_beginning_pressure"
                android:gravity="center"
                android:layout_width="0dip"
                android:layout_height="wrap_content"
                android:layout_weight="2" />
            <TextView
                android:id="@+id/hdrEndingPressure"
                android:text="@string/hdr_ending_pressure"
                android:gravity="center"
                android:layout_width="0dip"
                android:layout_height="wrap_content"
                android:layout_weight="2" />
        </TableRow>
    </androidx.cardview.widget.CardView>
</layout>