<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <data>
        <variable
            name="calculateDaltonLaw"
            type="ca.myairbuddyandi.CalculateDaltonLaw"/>
    </data>
    <RelativeLayout
        android:id="@+id/calculate_dalton_law_activity"
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:focusable="true"
        android:focusableInTouchMode="true">
        <LinearLayout
            android:id="@+id/buttonRow"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:orientation="horizontal"
            android:weightSum="8">
            <Button
                android:id="@+id/calculate_partial_pressure_button"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/button_partial_pressure"
                android:background="@color/very_light_blue"
                android:layout_weight="3"
                style="?android:attr/borderlessButtonStyle"/>
            <Button
                android:id="@+id/calculate_best_mix_button"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/button_best_mix"
                android:background="@color/very_light_blue_tinted"
                android:layout_weight="2"
                style="?android:attr/borderlessButtonStyle"/>
            <Button
                android:id="@+id/calculate_mod_button"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/button_mod"
                android:background="@color/theme_myapp_primary"
                android:layout_weight="1"
                style="?android:attr/borderlessButtonStyle"/>
            <Button
                android:id="@+id/calculate_end_button"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/button_end"
                android:background="@color/teal"
                android:layout_weight="1"
                android:enabled="false"
                android:visibility="gone"
                style="?android:attr/borderlessButtonStyle"/>
            <Button
                android:id="@+id/clear_button"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="@string/button_clear"
                android:background="@color/white"
                android:layout_weight="2"
                style="?android:attr/borderlessButtonStyle"/>
        </LinearLayout>
        <TableLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_above="@+id/buttonRow">
