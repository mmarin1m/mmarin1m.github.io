                android:id="@+id/TableRowHeader"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:stretchColumns="*"
                android:weightSum="3"
                android:paddingTop="@dimen/activity_vertical_margin"
                android:orientation="horizontal"
                tools:ignore="UselessParent">
                <TextView
                    android:id="@+id/defaultUnit"
                    android:text="@string/lbl_imperial"
                    android:textStyle="bold"
                    android:gravity="center"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1" />
                <ImageView
                    android:id="@+id/switchUnit"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center|center"
                    app:srcCompat="@drawable/ic_switch"
                    android:contentDescription="@string/cd_switch_unit"
                    app:tint="@color/theme_myapp_action_bar" />
                <TextView
                    android:id="@+id/otherUnit"
                    android:text="@string/lbl_metric"
                    android:textStyle="bold"
                    android:gravity="center"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1" />
            </TableRow>
            <TableRow
                android:id="@+id/TableRowSalinity"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:stretchColumns="*"
                android:weightSum="6"
                android:paddingTop="0dp"
                android:orientation="horizontal"
                tools:ignore="UselessParent">
                <!-- NOTE: Do not remove Unknown attribute android:selectedItemPosition-->
                <fr.ganfra.materialspinner.MaterialSpinner
                    android:id="@+id/defaultSalinity"
                    android:layout_width="0dp"
                    android:layout_height="50dp"
                    android:selectedItemPosition="@={calculateDaltonLaw.defaultSalinityPosition}"
                    android:layout_weight="2"
                    app:ms_multiline="false"
                    app:ms_enableFloatingLabel="false"
                    app:ms_enableErrorLabel="false"
                    app:setOnItemSelectedListener="@{calculateDaltonLaw.getOnSpinnerChangedDefaultSalinity}"
                    app:adapter="@{calculateDaltonLaw.AdapterDefaultSalinity}"
                    />
                <TextView
                    android:id="@+id/hdrSalinityLbl"
                    android:text="@string/hdr_salinity"
                    android:gravity="center|center"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="2" />
